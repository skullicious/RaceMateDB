@model IPagedList<CourseModel>

@{
    ViewBag.Title = "Courses";
}

<h2>Courses</h2>

<form method="get" action="@Url.Action("Index")"
      data-rmdb-ajax="true" data-rmdb-target="#courseList">

    <input type="search" name="searchTerm" data-rmdb-autocomplete="@Url.Action("Autocomplete")" />
    <input type="submit" value="Search By Name" />
</form>




<br />



@Html.Partial("_Courses", Model)