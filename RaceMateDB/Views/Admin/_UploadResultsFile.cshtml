@model IPagedList<ResultModel>

<div id="uploadResultList">

    <table class="table">
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.FirstOrDefault().Position)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstOrDefault().RiderName)
            </th>

            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.Position)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RiderName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "DeleteResultFromSession", new { name = item.RiderName })
                </td>
            </tr>
        }

    </table>


    <div class="pagedList" data-rmdb-target="#uploadResultList">
        @Html.PagedListPager(Model, page => Url.Action("UploadResultsFile", new { page }),
       PagedListRenderOptions.EnableUnobtrusiveAjaxReplacing(new AjaxOptions()
    { HttpMethod = "GET", UpdateTargetId = "uploadResultList"}))

    </div>

</div>